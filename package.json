{
    "name": "compo-layout",
    "version": "0.8.7",
    "homepage": "https://github.com/atmajs/compo-layout",
    "authors": [
        "Alex Kit <alex.kit@atmajs.com>"
    ],
    "description": "MaskJS Layout components (Atma.js)",
    "main": "lib/layout.js",
    "license": "MIT",

    "devDependencies": {
        "atma": ">0.9.26",
        "atma-loader-babel": ">0.0.0"
    },
    "scripts": {
        "test": "atma test"
    },
    "atma": {
        "plugins": [
            "atma-loader-babel"
        ],
        "settings": {
            "atma-loader-babel": {
                "extensions": [
                    "es6",
                    "test"
                ]
            }
        }
    }
}